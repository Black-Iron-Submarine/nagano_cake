
  <p>id</p>
  <p>商品名</p>
  <p>税抜価格</p>
  <P>ジャンル</P>
  <p>ステータス</p>
<% @items.each do |item| %>
  
  <span><%= item.id %></span>
  <span><%= item.name %></span>
  <span><%= item.price_without_tax %></span>
  <span><%= item.genre_id %></span>
  <span>
    <% if item.sales_status %>販売中
    <% else %>販売停止中
    <% end %>
  </span>
<% end %>